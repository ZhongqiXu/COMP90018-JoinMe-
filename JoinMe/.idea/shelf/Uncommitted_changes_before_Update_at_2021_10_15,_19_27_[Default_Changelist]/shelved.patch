+package com.comp90018.JoinMe;
+
+import android.content.Intent;
+import android.os.Bundle;
+import android.view.View;
+import android.widget.AdapterView;
+import android.widget.ArrayAdapter;
+import android.widget.EditText;
+import android.widget.ListView;
+import android.widget.Toast;
+
+import androidx.appcompat.app.AppCompatActivity;
+
+import com.google.firebase.auth.FirebaseAuth;
+
+import java.util.ArrayList;
+
+public class ActivityListActivity extends AppCompatActivity {
+    private FirebaseAuth mAuth;
+    EditText title;
+    ListView myListview;
+    ArrayList<String> arrayList = new ArrayList<>();
+
+    @Override
+    protected void onCreate(Bundle savedInstanceState) {
+        super.onCreate(savedInstanceState);
+        setContentView(R.layout.activity_list);
+        ArrayAdapter arrayAdapter = new ArrayAdapter(this, android.R.layout.simple_list_item_1,arrayList);
+
+        myListview = (ListView) findViewById(R.id.listview);
+        arrayList.add("coffee");
+        arrayList.add("painting");
+
+        myListview.setAdapter(arrayAdapter);
+
+        myListview.setOnItemClickListener(new AdapterView.OnItemClickListener() {
+            @Override
+            public void onItemClick(AdapterView<?> adapterView, View view, int i, long l) {
+                Toast.makeText(ActivityListActivity.this, "clicked item" + i + arrayList.get(i), Toast.LENGTH_SHORT).show();
+                startActivity(new Intent(ActivityListActivity.this,MyActivityActivity.class));
+            }
+        });
+    }
+}
